{"f":"./CC-MAIN-20211023064718-20211023094718-00225.warc.gz","o":148541960,"s":2411,"rs":4590,"u":"https://188.166.151.244/wap/?register","c":"text/html","ts":"2021-10-23T06:58:41Z","collection":"cc21","lang":["fin_Latn","xho_Latn","gle_Latn"],"prob":[0.7135,0.1342,0.0362],"text":"© 2018-2021 ITEAM4D","seg_langs":["gla_Latn"],"robotstxt":"allowed","id":"96d1a0324a7e1999351a92507bed9d50","filter":"word_avg_5","pii":[],"doc_scores":[0.0,0.0,10.0,4.2,0.0,0.0,10.0,0.0,0.0]}

