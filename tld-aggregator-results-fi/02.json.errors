{"f":"./CC-MAIN-20180525141236-20180525161236-00537.warc.gz","o":4756487,"s":2758,"rs":8284,"u":"http://96.125.197.241:88/address/CRKU85FxPDRFhysVnGfjLto6ao2AuKuwhV","c":"text/html","ts":"2018-05-25T14:42:03Z","collection":"cc18","lang":["fin_Latn","slk_Latn","hrv_Latn"],"prob":[0.5588,0.0679,0.047],"text":"CRKU85FxPDRFhysVnGfjLto6ao2AuKuwhV","seg_langs":["glg_Latn"],"robotstxt":"allowed","id":"981c5e651916bc48d957fc13c73289ab","filter":"word_avg_5","pii":[],"doc_scores":[0.0,0.0,10.0,0.0,10.0,6.1,10.0,0.0,0.0]}

